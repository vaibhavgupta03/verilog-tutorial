S_000001a63668bc60 .scope module, "testcounter" "testcounter" 2 1;
 .timescale 0 0;
v000001a63668b7f0_0 .var "clk", 0 0;
v000001a6365b30e0_0 .net "count", 31 0, v000001a636582bf0_0;  1 drivers
v000001a6365b3180_0 .var "rst", 0 0;
S_000001a63668b5c0 .scope module, "DUT" "counter16" 2 11, 3 3 0, S_000001a63668bc60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 32 "count";
v000001a63668bdf0_0 .net "clk", 0 0, v000001a63668b7f0_0;  1 drivers
v000001a636582bf0_0 .var "count", 31 0;
v000001a63668b750_0 .net "rst", 0 0, v000001a6365b3180_0;  1 drivers
E_000001a63668c600 .event posedge, v000001a63668bdf0_0;
    .scope S_000001a63668b5c0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v000001a636582bf0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_000001a63668b5c0;
T_1 ;
    %wait E_000001a63668c600;
    %load/vec4 v000001a63668b750_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v000001a636582bf0_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v000001a636582bf0_0;
    %addi 1, 0, 32;
    %assign/vec4 v000001a636582bf0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_000001a63668bc60;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a63668b7f0_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v000001a63668b7f0_0;
    %inv;
    %store/vec4 v000001a63668b7f0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_000001a63668bc60;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "counter16.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_000001a63668bc60 {0 0 0};
    %vpi_call 2 16 "$monitor", $time, " count is %d and rst is %d", v000001a6365b30e0_0, v000001a6365b3180_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001a6365b3180_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a6365b3180_0, 0, 1;
    %delay 290, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001a6365b3180_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
